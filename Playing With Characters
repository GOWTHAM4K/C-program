#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch[10],s[100],sen[100];
    scanf("%s",&ch);
    scanf("\n%[^\n]%*c",s);
    scanf("\n%[^\n]%*c",sen);
    printf("%s",ch);
    printf("\n%s",s);
    printf("\n%s",sen);
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
